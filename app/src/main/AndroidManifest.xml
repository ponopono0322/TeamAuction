<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamauction">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/Theme.TeamAuction">
        <activity
            android:name=".CharacterSelectActivity"
            android:exported="false" />
        <activity
            android:name=".PublisherLoginActivity"
            android:exported="false" />
        <activity
            android:name=".AddAccountActivity"
            android:exported="false" />
        <activity
            android:name=".PopupActivity"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name=".PasswordActivity"
            android:exported="false" />
        <activity
            android:name=".SignupActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="false" />
        <activity
            android:name=".StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--여기서부터 규인님이 만드신 것들-->
        <activity android:name=".AuctionScreen"
            android:exported="false"/>
        <activity android:name=".ItemCheckScreen"
            android:exported="false"/>
        <activity android:name=".SellingItemScreen"
            android:exported="false"/>
        <activity android:name=".BuyingScreen"
            android:exported="false"/>

    </application>



</manifest>